/* 弹窗-淡入淡出 */
.fadeBg{position:fixed;width:100%;height:100%;background-color:rgba(0,0,0,0.5);left:0;top:0;z-index:9990}
.pupfade-box{position:fixed;left:50%;top:50%;z-index:9999;background-color:#fff;transform:translate(-50%,-50%);padding:30rpx;}
/* 淡入动画 */
.fadeIn{animation:fadeIn .2s linear 0s 1;animation-fill-mode:forwards;opacity:0;}
@keyframes fadeIn{from {opacity:0;}to {opacity:1;}}
@-webkit-keyframes fadeIn{from {opacity:0;}to {opacity:1;}}
/*淡出动画 */
.fadeOut{opacity:0;animation:fadeOut .2s linear 0s 1;animation-fill-mode:forwards;}
@keyframes fadeOut{from {opacity:1;}to {opacity:0;}}
@-webkit-keyframes fadeOut{from {opacity:1;}to {opacity:0;}}
.no-tip{text-align:center;padding:30rpx 0;color:#666} 
/* 弹窗-底部 */
.pupBt,.pupBg{transition:all ease .3s;width:100%;position:fixed;left:0;bottom:0;}
.pupBt{background-color:#fff;padding-left:30rpx !important;padding-right:30rpx !important;z-index:9999;height:300rpx;}
.pupBg{z-index:9998;background-color:rgba(0,0,0,0.3);opacity:0;height:100%}
/* 底部-出现 */
.fadeUp{animation:fadeUp 0.2s ease 0s 1;-webkit-animation:fadeUp 0.2s ease 0s 1;animation-fill-mode:forwards;-webkit-animation-fill-mode:forwards;}
@keyframes fadeUp{ from{transform:translateY(110%)}  to{transform:translateY(0)} }
@-webkit-keyframes fadeUp{ from{transform:translateY(110%)}  to{transform:translateY(0)} }
/* 底部-消失 */
.fadeDown{animation:fadeDown 0.2s ease 0s 1;-webkit-animation:fadeDown 0.2s ease 0s 1;animation-fill-mode:forwards;-webkit-animation-fill-mode:forwards;}
@keyframes fadeDown{ from{transform:translateY(0)}  to{transform:translateY(110%)} }
@-webkit-keyframes fadeDown{ from{transform:translateY(0)}  to{transform:translateY(110%)} }

/* 弹窗右边-出来 */
.Rishow{animation:Rishow .2s linear 0s 1;animation-fill-mode:forwards;right:-105%}
@keyframes Rishow{from {right:-105%}to {right:0}}
@-webkit-keyframes Rishow{from {right:-105%}to {right:0}}
/* 弹窗右边-消失 */
.Rihide{animation:Rihide .2s linear 0s 1;animation-fill-mode:forwards;right:-105%}
@keyframes Rihide{from {right:0}to {right:-105%}}
