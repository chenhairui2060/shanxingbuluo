.loading3 {width: 20rpx;height: 20rpx;background: #999;border-radius: 50%;margin-left: 15rpx;display: inline-block;}
.loading3_1 {-webkit-animation: loading3 1s linear infinite alternate;animation: loading3 1s linear infinite alternate;}
.loading3_2 {-webkit-animation: loading3 1s linear infinite alternate;animation: loading3 1s linear infinite alternate;animation-delay: 0.5s;}
.loading3_3 {-webkit-animation: loading3 1s linear infinite alternate;animation: loading3 1s linear infinite alternate;animation-delay: 1s;}
.loading3_4 {-webkit-animation: loading3 1s linear infinite alternate;animation: loading3 1s linear infinite alternate;animation-delay: 1.5s;}
@-webkit-keyframes loading3 {
  0% {transform: scale(1);}
  100% {transform: scale(0);}
}
@keyframes loading3 {
  0% {transform: scale(1);}
  100% {transform: scale(0);}
}

